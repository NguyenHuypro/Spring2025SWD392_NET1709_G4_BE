            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "PackageDTO": {
        "required": [
          "packageName",
          "packagePrice"
        ],
        "type": "object",
        "properties": {
          "packageName": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "packagePrice": {
            "type": "number",
            "format": "double"
          },
          "serviceIds": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "uuid"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RegisterDTO": {
        "type": "object",
        "properties": {
          "fullName": {
            "type": "string",
            "nullable": true
          },
          "phone": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          },
          "passwordConfirmed": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ScheduleDTO": {
        "type": "object",
        "properties": {
